<StripLayouts
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<StripLayout Name="Default" ArrowType="Direction">
		<StripLayoutSections>
			<StripLayoutSection FlowDirection="Horizontal" Border="All">
				<StripLayoutSubsections>
					<StripLayoutSection FlowDirection="Horizontal">
						<StripLayoutLines>
							<StripLayoutLine>
								<StripItems>
									<StripItem Type="FDRState" LeftClick="Strip_State" MinLength="3" MaxLength="3"/>								
									<StripItem Type="ControllingSector" LeftClick="Strip_Controller" MaxLength="7" MinLength="7"/>
									<StripItem Type="AircraftType"	MinLength="5" MaxLength="5"/>
									<StripItem Type="NAT_Popup" MinLength="5"/>
									<StripItem Type="PRL" LeftClick="Label_PRL_Popup" MinLength="4"/>
								</StripItems>
							</StripLayoutLine>
							<StripLayoutLine>
								<StripItems>
									<StripItem Type="Callsign" LeftClick="Strip_Select" MiddleClick="Strip_Delete" RightClick="Strip_Cock" MaxLength="10" MinLength="10"/>
									<StripItem Type="Departure" MinLength="5" MaxLength="5"/>
									<StripItem Type="Destination" LeftClick="Label_ACID_Menu" MiddleClick="Strip_Extend" MinLength="5" MaxLength="5"/>
									<StripItem Type="NAT_CFL" LeftClick="Label_CFL_Popup" RightClick="Strip_CFL_Expand" MinLength="4"/>
									<StripItem Type="NAT_Mach" MinLength="5" MaxLength="5"/>
								</StripItems>
							</StripLayoutLine>
						</StripLayoutLines>
					</StripLayoutSection>
                    <StripLayoutSection FlowDirection="Horizontal" Border="All">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="0" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="0" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="1" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="1" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="2" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="2" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="3" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="3" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="4" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="4" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="5" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="5" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="6" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="6" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Horizontal" Border="Right">
                    	<StripLayoutLines>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="Point" LeftClick="Strip_Point" MiddleClick="Strip_MPR_Arm" RightClick="Strip_Point_Expand" PointIndex="7" MaxLength="6" MinLength="6" />
                        		</StripItems>
                        	</StripLayoutLine>
                        	<StripLayoutLine>
                        		<StripItems>
                            		<StripItem Type="ETOP" LeftClick="Strip_ETO" PointIndex="7" MinLength="4" />
                          		</StripItems>
                        	</StripLayoutLine>
                      	</StripLayoutLines>
                    </StripLayoutSection>
					<StripLayoutSection FlowDirection="Vertical" Border="Vertical">
						<StripLayoutLines>
							<StripLayoutLine>
								<StripItems>
									<StripItem Type="GlobalOpsInfo" LeftClick="Strip_GlobalOps" MaxLength="15" MinLength="15"/>
								</StripItems>
							</StripLayoutLine>
							<StripLayoutLine Border="Top">
								<StripItems>
									<StripItem Type="LocalOpsInfo" LeftClick="Strip_LocalOps" MaxLength="15" MinLength="15"/>
								</StripItems>
							</StripLayoutLine>
						</StripLayoutLines>
					</StripLayoutSection>
				</StripLayoutSubsections>
			</StripLayoutSection>
		</StripLayoutSections>
	</StripLayout>
</StripLayouts>